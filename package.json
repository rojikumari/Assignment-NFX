{
  "name": "react-web-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.dev react-app-rewired start",
    "start:qa": "env-cmd -f .env.qa react-app-rewired start",
    "start:stage": "env-cmd -f .env.staging react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "env-cmd -f .env.dev react-app-rewired build",
    "build:stage": "env-cmd -f .env.staging react-app-rewired build",
    "build:qa": "env-cmd -f .env.qa react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "prettier:write": "npx prettier --write **/*.{js,jsx,json} && npx prettier --write *.{js,jsx,json}"
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^10.0.0",
    "formik": "^2.2.9",
    "global": "^4.4.0",
    "lodash.mergewith": "^4.6.2",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-jest": "^27.2.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.2.5",
    "prettier": "2.4.1",
    "react-app-rewired": "^2.1.8"
  }
}
